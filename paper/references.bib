@article{valladasDatingLowerMiddle2013,
  title = {Dating the {{Lower}} to {{Middle Paleolithic}} Transition in the {{Levant}}: {{A}} View from {{Misliya Cave}}, {{Mount Carmel}}, {{Israel}}},
