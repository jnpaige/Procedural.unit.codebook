@article{uominiSharedBrainLateralization2013,
  title = {Shared {{Brain Lateralization Patterns}} in {{Language}} and {{Acheulean Stone Tool Production}}: {{A Functional Transcranial Doppler Ultrasound Study}}},
  shorttitle = {Shared {{Brain Lateralization Patterns}} in {{Language}} and {{Acheulean Stone Tool Production}}},
