@article{watersRedatingFellCave2015,
  title = {Redating {{Fell}}'s {{Cave}}, {{Chile}} and the {{Chronological Placement}} of the {{Fishtail Projectile Point}}},
  author = {Waters, Michael R. and Amorosi, Thomas and Stafford, Thomas W.},
  year = {2015},
