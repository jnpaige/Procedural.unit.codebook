@article{ortmanPreHistoryUrbanScaling2014,
  title = {The {{Pre}}-{{History}} of {{Urban Scaling}}},
  author = {Ortman, Scott G. and Cabaniss, Andrew H. F. and Sturm, Jennie O. and Bettencourt, Lu{\'i}s M. A.},
  editor = {Shennan, Stephen},
