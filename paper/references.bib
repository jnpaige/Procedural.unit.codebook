@article{erwinDISPARITYMORPHOLOGICALPATTERN2007,
  title = {{{DISPARITY}}: {{MORPHOLOGICAL PATTERN AND DEVELOPMENTAL CONTEXT}}},
  shorttitle = {{{DISPARITY}}},
  author = {Erwin, Douglas H.},
  year = {2007},
