@article{wangTrackingHumanPopulation2020,
  title = {Tracking Human Population Structure through Time from Whole Genome Sequences},
  author = {Wang, Ke and Mathieson, Iain and O'Connell, Jared and Schiffels, Stephan},
  year = {2020},
  month = mar,
  volume = {16},
  pages = {e1008552},
