  file = {C\:\\Users\\Jon Paige\\Desktop\\Zotero data\\storage\\DI9GT84Y\\Stepanchuk et al. - 2017 - The last Neanderthals of Eastern Europe Micoquian.pdf},
  journal = {Quaternary International},
