  journal = {Engineering Structures},
  language = {en}
}

@article{wangComparisonHandaxesBose2012,
  title = {Comparison of {{Handaxes}} from {{Bose Basin}} ({{China}}) and the {{Western Acheulean Indicates Convergence}} of {{Form}}, {{Not Cognitive Differences}}},
  author = {Wang, Wei and Lycett, Stephen J. and {von Cramon-Taubadel}, Noreen and Jin, Jennie J. H. and Bae, Christopher J.},
