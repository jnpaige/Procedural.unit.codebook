@article{manicaEffectAncientPopulation2007,
  title = {The Effect of Ancient Population Bottlenecks on Human Phenotypic Variation},
  author = {Manica, Andrea and Amos, Bill and Balloux, Fran{\c c}ois and Hanihara, Tsunehiko},
  year = {2007},
