@article{rodriguezrodriguezEstudioPicosTallados2012,
  title = {{Estudio de los picos tallados de la \'epoca preeuropea de Gran Canaria. Un ejemplo de especializaci\'on en el trabajo a partir de las evidencias recuperadas en la cantera de molinos de Monta\~na Quemada}},
  author = {Rodr{\'i}guez Rodr{\'i}guez, Amelia C. and Francisco Ortega, Mar{\'i}a Isabel},
  year = {2012},
