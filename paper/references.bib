  doi = {10.1016/j.yqres.2007.10.012},
  abstract = {Pollen, spore, macrofossil and stable isotope (C and N) analyses from a 266-cm sediment core collected from a swamp on the Eastern Arc Mountains, Tanzania, are used to reconstruct vegetation and environmental history. An estimated time scale based on five 14C ages records approximately 38,000 yr. This palaeorecord is the first from this biodiversity hotspot and importantly extends through the last glacial maximum (LGM). The altitudinal transition from montane to upper montane forest shifted from 1700\textendash 1800 m (38,000 14C yr BP) to 1800\textendash 1900 m (35,000\textendash 29,000 14C yr BP). From 29,000 to 10,000 14C yr BP, it shifted from 1850\textendash 1950 m across the LGM to 1750\textendash 1800 m (during 10,000\textendash 3500 14C yr BP), and to present-day elevations at 2000 m during the last 3500 14C yr BP. The relative ecosystem stability across the LGM may be explained by the Indian Ocean's influence in maintaining continuous moist forest cover during a period of East African regional climate aridity. During the late Holocene, presence of abundant coprophilous fungi and algal blooms demonstrates increasing human impact. Neurospora spores indicate frequent fires, coinciding with clear signals of decline in Podocarpus and Psychotria trees that possibly represent selective logging.},
  file = {C\:\\Users\\Jon Paige\\Desktop\\Zotero data\\storage\\25C67VWI\\Mumbi et al. - 2008 - Late Quaternary vegetation reconstruction from the.pdf},
  journal = {Quaternary Research},
  language = {en},
  number = {2}
}

@article{mundayLevantineMousterianTechnological1979,
