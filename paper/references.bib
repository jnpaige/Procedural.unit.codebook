  author = {Stoetzel, Emmanuelle and Campmas, Emilie and Michel, Patrick and Bougariane, Bouchra and Ouchaou, Brahim and Amani, Fethi and El Hajraoui, Mohamed Abdejalil and Nespoulet, Roland},
