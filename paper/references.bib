}

@article{schifferArchaeologicalContextSystemic1972,
  title = {Archaeological {{Context}} and {{Systemic Context}}},
  author = {Schiffer, Michael B.},
  year = {1972},
  month = apr,
  volume = {37},
  pages = {156--165},
  issn = {0002-7316, 2325-5064},
