}

@article{presnyakovaDocumentingDifferencesEarly2015,
  title = {Documenting {{Differences}} between {{Early Stone Age Flake Production Systems}}: {{An Experimental Model}} and {{Archaeological Verification}}},
  shorttitle = {Documenting {{Differences}} between {{Early Stone Age Flake Production Systems}}},
  author = {Presnyakova, Darya and Archer, Will and Braun, David R. and Flear, Wesley},
