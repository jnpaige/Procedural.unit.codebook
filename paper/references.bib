}

@book{loveMemeDevelopmentStructure2019,
  title = {Beyond the {{Meme}}: {{Development}} and {{Structure}} in {{Cultural Evolution}}},
  shorttitle = {Beyond the {{Meme}}},
  editor = {Love, Alan C. and Wimsatt, William C.},
  year = {2019},
  month = sep,
  publisher = {{University of Minnesota Press}},
  doi = {10.5749/j.ctvnp0krm},
  abstract = {In principle, new theoretical structures in physics, unlike arches and other architectural structures, could be erected without the use of any scaffolds. After all, that is essentially how the four-dimensional formalism of special relativity, the curved space-times of general relativity, and the Hilbert-space formalism of quantum mechanics are introduced in modern textbooks. Historically, however, such structures, like arches, were originally erected on top of elaborate scaffolds provided by the structures they eventually either partially or completely replaced. The metaphor of arches and scaffolds highlights the remarkable degree of continuity in instances of theory change that, at first sight, look strikingly discontinuous. After putting to rest some historiographical scruples about the metaphor and presupposing as little knowledge of the relevant physics and mathematics as possible, I describe how some key steps in the development of relativity and quantum theory in the early decades of the 20th century are captured quite naturally in terms of arches and scaffolds. Given how easy it is to find examples of this kind, I argue that it may be worthwhile to further analyze this pattern of theory change with the help of some of Stephen Jay Gould's ideas about evolutionary biology, especially his notion of constraints.},
  file = {C\:\\Users\\Jon Paige\\Desktop\\Zotero data\\storage\\6GCIFEUK\\Love and Wimsatt - 2019 - Beyond the Meme Development and Structure in Cult.pdf},
  isbn = {978-1-4529-6156-9 978-1-5179-0690-0},
  language = {en}
}

