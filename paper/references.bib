@article{gunzAustralopithecusAfarensisEndocasts2020,
  title = {{\emph{Australopithecus Afarensis}} Endocasts Suggest Ape-like Brain Organization and Prolonged Brain Growth},
  author = {Gunz, Philipp and Neubauer, Simon and Falk, Dean and Tafforeau, Paul and Le Cabec, Adeline and Smith, Tanya M. and Kimbel, William H. and Spoor, Fred and Alemseged, Zeresenay},
