  author = {Valladas, H{\'e}l{\`e}ne and Mercier, Norbert and Hershkovitz, Israel and Zaidner, Yossi and Tsatskin, Alexander and Yeshurun, Reuven and Vialettes, Laurence and Joron, Jean-Louis and Reyss, Jean-Louis and {Weinstein-Evron}, Mina},
