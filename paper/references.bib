@article{riedeLaacherSeeeruption122008,
  title = {The {{Laacher See}}-Eruption (12,920 {{BP}}) and Material Culture Change at the End of the {{Aller\o d}} in {{Northern Europe}}},
  author = {Riede, Felix},
  year = {2008},
  month = mar,
  volume = {35},
  pages = {591--599},
  issn = {0305-4403},
  doi = {10.1016/j.jas.2007.05.007},
  abstract = {A number of recent papers have argued that summed probability distributions of radiocarbon dates calibrated with the CALPAL software package can be used to identify population trends in prehistory. For instance, Gamble et~al. (Gamble, C., Davies, W., Pettitt, P., Richards, M., 2004. Climate change and evolving human diversity in Europe during the last glacial. Philosophical Transactions of the Royal Society of London, Series B 359, 243\textendash 254; Gamble, C., Davies, W., Pettitt, P., Richards, M., 2005. The archaeological and genetic foundations of the European population during the Late Glacial: implications for 'agricultural thinking'. Cambridge Archaeological Journal 15, 193\textendash 223.) have demonstrated that during the European Late Glacial, demography was more variable than hitherto acknowledged. Building on this work, this paper presents evidence that the large, but so far largely ignored eruption of the Laacher See-volcano, located in present-day western Germany and dated to 12,920 BP, had a dramatic impact on forager demography all along the northern periphery of Late Glacial settlement and precipitated archaeologically visible cultural change. In Southern Scandinavia, these changes took the form of technological simplification, the loss of bow-and-arrow technology, and coincident with these changes, the emergence of the regionally distinct Bromme culture. Groups in north-eastern Europe appear to have responded to the eruption in similar ways, but on the British Isles and in the Thuringian Basin populations contracted or relocated, leaving these areas largely depopulated already before the onset of the Younger Dryas/GS-1 cooling. Demographic models are used to link these changes to the Laacher See-eruption and this research demonstrates that we cannot sideline catastrophic environmental change in our reconstructions of prehistoric culture history.},
  file = {C\:\\Users\\Jon Paige\\Desktop\\Zotero data\\storage\\IA7LFCTN\\Riede - 2008 - The Laacher See-eruption (12,920 BP) and material .pdf;C\:\\Users\\Jon Paige\\Desktop\\Zotero data\\storage\\77ZRZ82B\\S0305440307001008.html},
  journal = {Journal of Archaeological Science},
  keywords = {Bromme culture,Laacher See-eruption,Late Glacial,Population history,Radiocarbon dates},
  language = {en},
  number = {3}
}

