@article{brandtNewMISRadiocarbon2017,
  title = {A New {{MIS}} 3 Radiocarbon Chronology for {{Mochena Borago Rockshelter}}, {{SW Ethiopia}}: {{Implications}} for the Interpretation of {{Late Pleistocene}} Chronostratigraphy and Human Behavior},
  shorttitle = {A New {{MIS}} 3 Radiocarbon Chronology for {{Mochena Borago Rockshelter}}, {{SW Ethiopia}}},
