@article{kaufuluFormationPreservationEarlier1987,
  title = {Formation and {{Preservation}} of {{Some Earlier Stone Age Sites}} at {{Koobi Fora}}, {{Northern Kenya}}},
  author = {Kaufulu, Zefe M.},
  year = {1987},
  volume = {42},
  pages = {23--33},
  publisher = {{South African Archaeological Society}},
  issn = {0038-1969},
  doi = {10.2307/3887770},
  abstract = {Over sixty Earlier Stone Age sites occur in several different sedimentary contexts at Koobi Fora, northern Kenya. Fourteen of these are located in the Karari area. They comprise a sample of archaeological occurrences from clearly diagnosed sedimentary subenvironments. Assemblages from thirteen of these sites are classified as Oldowan and one as Acheulian. They are stratified in sediments dated between 1,0 million and 2,0 million years. Sedimentological analyses of the contexts of the sites, combined with studies of modern analogues, provide information about the formation and preservation of sites in fluvial subenvironments. The results of modern environmental studies are used as a basis for relating the sedimentary processes to the sediment types and sedimentary features produced. They are applied in the geological analyses and interpretation of the fourteen sites. Two groups of sedimentary processes are particularly important in affecting the nature of archaeological sites -- running water and diagenesis. Running water rearranges and sorts most archaeological materials that are discarded on banks and floodbasins. Over time, it concentrates discards originally scattered on the streambeds of active channels through the mechanism of gravel-bar formation. Processes of diagenesis (including weathering, soil formation, bioturbation and chemical precipitation) also result in varying extents of vertical and horizontal displacements. The effects of running water on archaeological materials are greatest in sites located in active streambeds, less in sites located on banks and lev\'ees, and least in sites located in floodbasins.},
  journal = {The South African Archaeological Bulletin},
  number = {145}
}

