@article{erikssonSoilErosionHistory2000,
  title = {Soil Erosion History in Central {{Tanzania}} Based on {{OSL}} Dating of Colluvial and Alluvial Hillslope Deposits},
  author = {Eriksson, M.G and Olley, J.M and Payton, R.W},
