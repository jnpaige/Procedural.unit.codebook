@article{arnoldSettlementSubsistenceEarly2009,
  title = {Settlement and Subsistence among the {{Early Formative Gulf Olmec}}},
  author = {Arnold, Philip J.},
  year = {2009},
  month = dec,
  volume = {28},
  pages = {397--411},
  issn = {02784165},
  doi = {10.1016/j.jaa.2009.08.001},
  abstract = {Mounting archaeological evidence suggests that floodplain resources, not maize (Zea mays) agriculture, were instrumental in the emergence of Early Formative (ca. 1500\textendash 900 uncal BC) complexity across Mesoamerica's isthmian lowlands. The lion's share of these data derives from the Pacific side of the isthmus; discussions of the Early Formative Olmec along Mexico's southern Gulf lowlands have not kept pace. This paper presents settlement and subsistence data that highlight the role of floodplain resources in the development of Gulf Olmec politico-economic complexity. These data support a non-agricultural alternative to traditional models of Gulf Olmec emergence at San Lorenzo, the premier Early Formative Gulf lowlands center. Increased productivity of maize toward the end of the Early Formative period challenged San Lorenzo's extant politico-economic basis, bringing about a short-term, hyper-acceleration of elite competitive displays. Ultimately, the adoption of maize agriculture generated a reorganized Middle Formative period (ca. 900\textendash 400 uncal BC) landscape in and around San Lorenzo. This agrarian adjustment saw occupation move out of the floodplain and into the upland areas, a process sometimes characterized as a cataclysmic system collapse in the Coatzacoalcos basin.},
  file = {C\:\\Users\\Jon Paige\\Desktop\\Zotero data\\storage\\MTXB7MZG\\Arnold - 2009 - Settlement and subsistence among the Early Formati.pdf},
  journal = {Journal of Anthropological Archaeology},
  language = {en},
  number = {4}
}

