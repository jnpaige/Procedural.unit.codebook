  file = {C\:\\Users\\Jon Paige\\Desktop\\Zotero data\\storage\\WMPLSSFW\\Wirrmann et al. - 2006 - Late Holocene paleoenvironment in northern New Cal.pdf;C\:\\Users\\Jon Paige\\Desktop\\Zotero data\\storage\\T7DQD64M\\S0033589406000524.html},
