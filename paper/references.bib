@article{quinnPedogenicCarbonateStable2013,
  title = {Pedogenic Carbonate Stable Isotopic Evidence for Wooded Habitat Preference of~Early {{Pleistocene}} Tool Makers in the {{Turkana Basin}}},
  author = {Quinn, Rhonda L. and Lepre, Christopher J. and Feibel, Craig S. and Wright, James D. and Mortlock, Richard A. and Harmand, Sonia and Brugal, Jean-Philip and Roche, H{\'e}l{\`e}ne},
  year = {2013},
  month = jul,
  volume = {65},
  pages = {65--78},
  issn = {0047-2484},
  doi = {10.1016/j.jhevol.2013.04.002},
  abstract = {The origin and evolution of early Pleistocene hominin lithic technologies in Africa occurred within the context of savanna grassland ecosystems. The Nachukui Formation of the Turkana Basin in northern Kenya, containing Oldowan and Acheulean tool assemblages and fossil evidence for early members of Homo and Paranthropus, provides an extensive spatial and temporal paleosol record of early Pleistocene savanna flora. Here we present new carbon isotopic ({$\delta$}13CVPDB) values of pedogenic carbonates (68 nodules, 193 analyses) from the Nachukui Formation in order to characterize past vegetation structure and change through time. We compared three members (Kalochoro, Kaitio, and Natoo) at five locations spanning 2.4\textendash 1.4Ma and sampled in proximity to hominin archaeological and paleontological sites. Our results indicate diverse habitats showing a mosaic pattern of vegetation cover at each location yet demonstrate grassland expansion through time influenced by paleogeography. Kalochoro floodplains occurred adjacent to large river systems, and paleosols show evidence of C3 woodlands averaging 46\textendash 50\% woody cover. Kaitio habitats were located along smaller rivers and lake margins. Paleosols yielded evidence for reduced portions of woody vegetation averaging 34\textendash 37\% woody cover. Natoo environments had the highest percentage of grasslands averaging 21\% woody cover near a diminishing Lake Turkana precursor. We also compared paleosol {$\delta$}13CVPDB values of lithic archaeological sites with paleosol {$\delta$}13CVPDB values of all environments available to hominins at 2.4\textendash 1.4Ma in the Nachukui and Koobi Fora Formations. Grassy environments became more widespread during this interval; woody canopy cover mean percentages steadily decreased by 12\%. However, significantly more wooded savanna habitats were present in the vicinity of lithic archaeological sites and did not mirror the basin-wide trend of grassland spread. Hominin lithic archaeological sites consistently demonstrated woody cover circa 40\% throughout our study interval and were 4\textendash 12\% more woody than coeval basin environs. We propose that Turkana Basin early tool makers may have preferred a more wooded portion of the savanna ecosystem to reduce heat stress and to gain differential access to potable water, raw materials, animal carcasses, and edible plants.},
  file = {C\:\\Users\\Jon Paige\\Desktop\\Zotero data\\storage\\GEBMVQ5H\\S0047248413000997.html},
  journal = {Journal of Human Evolution},
  keywords = {Carbon isotopes,Paleolithic archaeological sites,Pleistocene East Africa,Savanna environments},
  language = {en},
  number = {1}
}

