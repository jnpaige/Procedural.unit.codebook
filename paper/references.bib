}

@article{wynnHowLevalloisReduction,
  title = {How {{Levallois Reduction Is Similar}} to, and {{Not Similar}} to, {{Playing Chess}}},
  author = {Wynn, Thomas and Coolidge, Frederick L},
  pages = {23},
  abstract = {Modern cognition is more than language and symbolism. One important component of modern thinking is expertise, exemplified best in expert performances in the arts, craft production, sport, medicine, and games such as chess. Expertise is driven by a cognitive system known as long-term working memory, in which retrieval structures held in long-term memory are activated in working memory. Here they enable rapid access to large bodies of procedural and declarative knowledge that have been acquired over years of practice. Although expertise underpins many of our most esteemed accomplishments, it is not reliant on either symbols or language. In the following essay we analyze one example of prehistoric expertise\textemdash Marjorie's core from Maastricht-Belvedere. From Nathan Schlanger's (1996) description of the cha\^ine op\'eratoire we have been able to identify many of the elements of the retrieval structure activated for knapping the core. In its overall organization this retrieval structure was no different from those deployed by modern artisans. With this analysis in hand we are then able to contrast this Levallois retrieval structure with one required for an earlier biface fa\c{c}onnage reduction, thereby tracing one link in the evolutionary sequence leading ultimately to modern expertise.},
  file = {C\:\\Users\\Jon Paige\\Desktop\\Zotero data\\storage\\6QZ63XGH\\Wynn and Coolidge - How Levallois Reduction Is Similar to, and Not Sim.pdf},
  language = {en}
}

