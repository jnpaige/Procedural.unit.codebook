  author = {Doronicheva, Ekaterina V. and Golovanova, Liubov V. and Doronichev, Vladimir B. and Nedomolkin, Andrey G. and Korzinova, Anastasiya S. and Tselmovitch, Vladimir A. and Kulkova, Marianna A. and Odinokova, Elena V. and Shirobokov, Ivan G. and Ivanov, Vadim V. and Nesmeyanov, Sergey A. and Voeykova, Olga A. and Muriy, Alexander A. and Tregub, Tamara F. and Volkov, Maksim A. and Shackley, M. Steven and Spasovskiy, Yuriy N.},
