@article{boedaLevalloisNonLevalloisBlade2013,
  title = {Levallois and Non-{{Levallois}} Blade Production at {{Shuidonggou}} in {{Ningxia}}, {{North China}}},
  author = {Bo{\"e}da, E. and Hou, Y.M. and Forestier, H. and Sarel, J. and Wang, H.M.},
