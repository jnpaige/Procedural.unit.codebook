@article{collardNicheConstructionToolkits2011,
  title = {Niche {{Construction}} and the {{Toolkits}} of {{Hunter}}\textendash{{Gatherers}} and {{Food Producers}}},
  author = {Collard, Mark and Buchanan, Briggs and Ruttle, April and O'Brien, Michael J.},
  year = {2011},
  month = sep,
  volume = {6},
  pages = {251--259},
  issn = {1555-5542, 1555-5550},
  doi = {10.1007/s13752-012-0034-6},
  abstract = {In the study reported here we examined the impact of population size and two proxies of risk of resource failure on the diversity and complexity of the food-getting toolkits of hunter\textendash gatherers and small-scale food producers. We tested three hypotheses: the risk hypothesis, the population-size hypothesis, and a hypothesis derived from niche construction theory. Our analyses indicated that the toolkits of hunter\textendash gatherers are more affected by risk than are the toolkits of food producers. They also showed that the toolkits of food producers are more affected by population size than are the toolkits of hunter\textendash gatherers. This pattern is inconsistent with the predictions of both the risk hypothesis and the populationsize hypothesis. In contrast, it is consistent with the predictions of the niche construction hypothesis. Our results indicate that niche construction has affected the evolution of technology in small-scale societies and imply that niche construction must be taken into account when seeking to understand technological variation among food producers and the technological changes that occurred in association with the various transitions to farming that have occurred over the last 10,000 years.},
  file = {C\:\\Users\\Jon Paige\\Desktop\\Zotero data\\storage\\MNMDY2VH\\Collard et al. - 2011 - Niche Construction and the Toolkits of Hunter–Gath.pdf},
  journal = {Biological Theory},
  language = {en},
  number = {3}
}

