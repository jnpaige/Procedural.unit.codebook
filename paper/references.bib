@article{scerriEarliestEvidenceStructure2014,
  title = {Earliest Evidence for the Structure of {{Homo}} Sapiens Populations in {{Africa}}},
  author = {Scerri, Eleanor M.L. and Drake, Nick A. and Jennings, Richard and Groucutt, Huw S.},
