@incollection{louysNeolithicDispersalImplications2018,
  title = {Neolithic Dispersal Implications of Murids from Late {{Holocene}} Archaeological and Modern Natural Deposits in the {{Talaud Islands}}, Northern {{Sulawesi}}},
  booktitle = {The {{Archaeology}} of {{Sulawesi}}: {{Current Research}} on the {{Pleistocene}} to the {{Historic Period}}},
  author = {Louys, Julien and Herrera, Michael and Hawkins, Stuart and Aplin, Ken and Reepmeyer, Christian and Hopf, Felicitas and Donnellan, Stephen C. and O'Connor, Sue and Tanudirjo, Daud A.},
