}

@article{marzkeEvolutionHumanHand2000,
  title = {Evolution of the Human Hand: Approaches to Acquiring, Analysing and Interpreting the Anatomical Evidence},
  shorttitle = {Evolution of the Human Hand},
  author = {Marzke, Mary W. and Marzke, R. F.},
