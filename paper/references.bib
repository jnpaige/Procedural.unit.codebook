@article{barnesDifferentialImpactsReligious2020,
  title = {Differential {{Impacts}} of {{Religious Cultural Competence}} on {{Students}}' {{Perceived Conflict}} with {{Evolution}} at an {{Evangelical University}}},
  author = {Barnes, M. Elizabeth and Werner, Ruth and Brownell, Sara E.},
  year = {2020},
