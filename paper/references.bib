@article{buskesEnculturedPrimateThresholds2019,
  title = {The {{Encultured Primate}}: {{Thresholds}} and {{Transitions}} in {{Hominin Cultural Evolution}}},
  shorttitle = {The {{Encultured Primate}}},
  author = {Buskes, Chris},
  year = {2019},
  month = mar,
  volume = {4},
  pages = {6},
  publisher = {{Multidisciplinary Digital Publishing Institute}},
  doi = {10.3390/philosophies4010006},
  abstract = {This article tries to shed light on the mystery of human culture. Human beings are the only extant species with cumulative, evolving cultures. Many animal species do have cultural traditions in the form of socially transmitted practices but they typically lack cumulative culture. Why is that? This discrepancy between humans and animals is even more puzzling if one realizes that culture seems highly advantageous. Thanks to their accumulated knowledge and techniques our early ancestors were able to leave their cradle in Africa and swarm out across the planet, thereby adjusting themselves to a whole range of new environments. Without culture this would have been impossible. So we may ask once again: if cumulative culture is so useful, why don\&rsquo;t other animals have it? In order to explain this mystery I won\&rsquo;t appeal to the major transitions in human evolution\&mdash;like walking upright, crafting stone tools and controlling fire, etc.\&mdash;because that would be question begging. Instead I try to unearth the mechanisms that caused those evolutionary turning points to occur in the first place. It seems that unlike other animals, humans are predisposed to efficiently acquire, store and transmit cultural information in such ways that our cultures can genuinely evolve.},
  copyright = {http://creativecommons.org/licenses/by/3.0/},
  file = {C\:\\Users\\Jon Paige\\Desktop\\Zotero data\\storage\\T97TIFZY\\Buskes - 2019 - The Encultured Primate Thresholds and Transitions.pdf;C\:\\Users\\Jon Paige\\Desktop\\Zotero data\\storage\\LWYXFPK7\\htm.html},
  journal = {Philosophies},
  keywords = {cultural evolution,cumulative culture,dual inheritance,gene–culture coevolution,memetics,universal Darwinism},
  language = {en},
  number = {1}
}

@article{buvitRadiocarbonDatesMicroblades2016,
  title = {Radiocarbon Dates, Microblades and {{Late Pleistocene}} Human Migrations in the {{Transbaikal}}, {{Russia}} and the {{Paleo}}-{{Sakhalin}}-{{Hokkaido}}-{{Kuril Peninsula}}},
  author = {Buvit, Ian and Izuho, Masami and Terry, Karisa and Konstantinov, Mikhail V. and Konstantinov, Aleksander V.},
  year = {2016},
  month = dec,
  volume = {425},
  pages = {100--119},
  issn = {10406182},
  doi = {10.1016/j.quaint.2016.02.050},
  abstract = {We compare late Pleistocene archaeological 14C databases from the Transbaikal, Russia and the PaleoSakhalin-Hokkaido-Kuril Peninsula (PSHK) to the appearance and disappearance of microblade technology for evidence of human migration. In the Transbaikal, we identify as many as three events at 24,830, 22,740, and \textasciitilde 15,000 cal bp, respectively. On the PSHK, we see migration at \textasciitilde 26,800, and likely again at \textasciitilde 13,000 cal bp. We offer an explanation for the Last Glacial Maximum origin of northeast Asian microblades that reconciles several major points of current competing theories. The later migration signals in both areas are weaker and associated with terminal Pleistocene (B\o lling-Aller\o d-Younger Dryas) environmental changes at the beginning of the Mesolithic Period in the Transbaikal and the Incipient-Jomon Period on PSHK. In the former, the second migration event may have been repopulation of an empty or sparsely inhabited area. In the latter, it appears to be population replacement when microblades completely disappear from the Peninsula without a reduction in the number of 14C dates.},
  file = {C\:\\Users\\Jon Paige\\Desktop\\Zotero data\\storage\\A9UQPQXH\\Buvit et al. - 2016 - Radiocarbon dates, microblades and Late Pleistocen.pdf},
  journal = {Quaternary International},
  language = {en}
}

