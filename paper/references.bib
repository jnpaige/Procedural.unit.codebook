@article{nasabLatePleistoceneDispersal2013,
  title = {Late {{Pleistocene}} Dispersal Corridors across the {{Iranian Plateau}}: {{A}} Case Study from {{Mirak}}, a {{Middle Paleolithic}} Site on the Northern Edge of the {{Iranian Central}} Desert ({{Dasht}}-e {{Kavir}})},
