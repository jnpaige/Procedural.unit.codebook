@incollection{crittendenShareNotShare2016,
  title = {To {{Share}} or {{Not}} to {{Share}}? {{Social Processes}} of {{Learning}} to {{Share Food Among Hadza Hunter}}-{{Gatherer Children}}},
  shorttitle = {To {{Share}} or {{Not}} to {{Share}}?},
  booktitle = {Social {{Learning}} and {{Innovation}} in {{Contemporary Hunter}}-{{Gatherers}}: {{Evolutionary}} and {{Ethnographic Perspectives}}},
  author = {Crittenden, Alyssa N.},
