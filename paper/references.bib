@article{mccoyStrategiesObtainingObsidian2014,
  title = {Strategies for {{Obtaining Obsidian}} in {{Pre}}-{{European Contact Era New Zealand}}},
  author = {McCoy, Mark D. and Carpenter, Jonathan},
  editor = {Petraglia, Michael D.},
  year = {2014},
