@article{robinsonPaleoenvironmentalChangeLate2016,
  title = {Paleoenvironmental Change in the Late {{Middle Pleistocene}}\textendash{{Holocene Kibish Formation}}, Southern {{Ethiopia}}: {{Evidence}} from Ungulate Isotopic Ecology},
  shorttitle = {Paleoenvironmental Change in the Late {{Middle Pleistocene}}\textendash{{Holocene Kibish Formation}}, Southern {{Ethiopia}}},
  author = {Robinson, Joshua R. and Rowan, John and Faith, J. Tyler and Fleagle, John G.},
  year = {2016},
  month = may,
  volume = {450},
  pages = {50--59},
  issn = {0031-0182},
  doi = {10.1016/j.palaeo.2016.02.049},
  abstract = {East Africa has produced the earliest record of Homo sapiens\textasciitilde 200ka and a punctuated record of Middle Stone Age and Later Stone Age behaviors. We lack, however, a detailed late Quaternary paleoenvironmental record for the region, particularly during humid periods. Without a regional record, hypotheses about the evolution and ecology of early Homo sapiens in East Africa remain vague and untestable. The Kibish Formation of southern Ethiopia presents a long, albeit punctuated, record of late Middle Pleistocene to Holocene faunal change in East Africa, which was deposited during humid periods. Here, we present oxygen and carbon stable isotope data of the Kibish ungulates and test whether there are environmental changes within the Kibish Formation. Significant differences in {$\delta$}18O enamel isotopes are consistent with more humid conditions during the Holocene-age Member IV (\textasciitilde 13\textendash 4ka) than either Pleistocene-age Member I (\textasciitilde 196ka) or Member III (\textasciitilde 104ka). Mesowear data document a shift toward more attritional wear among grazers in Member IV and are correlated with more depleted {$\delta$}18O enamel values, suggesting that the wear pattern shift is linked to the onset of more humid conditions during the Holocene. {$\delta$}13C enamel values show subtle variations through time, but do not suggest any major changes in diets. We propose that the paleoenvironmental differences evident in Member IV, based on {$\delta$}18O enamel values, mesowear, and bovid abundances, may be explained by cooler and wetter conditions at the beginning of the Holocene in the lower Omo Valley. The evidence suggesting that the Holocene humid phase is more pronounced than earlier humid phases may explain why arid-adapted grassland ungulates became extinct in East Africa by the Pleistocene\textendash Holocene transition, but persisted through previous humid phases of the late Quaternary.},
  file = {C\:\\Users\\Jon Paige\\Desktop\\Zotero data\\storage\\78DVS3QH\\Robinson et al. - 2016 - Paleoenvironmental change in the late Middle Pleis.pdf;C\:\\Users\\Jon Paige\\Desktop\\Zotero data\\storage\\9W8M4Z86\\S0031018216001516.html},
  journal = {Palaeogeography, Palaeoclimatology, Palaeoecology},
  keywords = {East Africa,Ethiopia,Isotopes,Late Quaternary,Mesowear,Ungulates},
  language = {en}
}

