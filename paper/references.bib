@article{yooExaminationChronotechnologicalFeatures2019,
  title = {Examination of the Chrono-Technological Features of the Handaxes from the {{Imjin}}-{{Hantan River Area}} in {{Korea}}},
  author = {Yoo, Yongwook},
  year = {2019},
  month = feb,
  volume = {503},
  pages = {97--104},
  issn = {10406182},
  doi = {10.1016/j.quaint.2018.10.039},
  abstract = {The Acheulian-like handaxe has been continuously discovered in the Imjin-Hantan River Area (the IHRA) of Korea since 1970s and its age has been claimed to be Middle Pleistocene based on its morphological similarity to Mode II industry as well as several K-Ar dates of lava flow far below the artifact horizon. During the last decade, new data and new perspectives continuously suggested the IHRA handaxe is quite younger than previously expected and are not coeval with Acheulian handaxes; this furnished with an excellent opportunity to re-examine the technological characteristics of palaeolithic assemblages in this area. Critically examining the result of past research as well as reviewing available chronometric dates, the IHRA handaxe is claimed to be far from the result of Acheulian acculturation during the Middle Pleistocene in the Far Eastern Asia. It was initially made considerably later than the termination of volcanic eruption; many chronometric dates indicate that the IHRA handaxe found above the basalt bed was made no earlier than MIS 4 and persisted until the end of MIS 3. This temporal range does not support that the IHRA handaxe is coeval and an out of Acheulian influence. An alternative position to this culture-oriented perspective is that the emergence of IHRA handaxe is engineered not solely by hominin migration or transmission of manufacturing ideas, but can be brought about by other human interference (e.g. reflecting the genetic flows in the course of micro-evolution among local hominin groups). A claim of independent emergence of Acheulian-like handaxe in the Far East area can open a venue to a more realistic idea that the materialistic expression of Pleistocene Old World is rather stochastic than axiomatic regardless of culture-based transmission of specific ideas.},
  file = {C\:\\Users\\Jon Paige\\Desktop\\Zotero data\\storage\\668XE4QW\\Yoo - 2019 - Examination of the chrono-technological features o.pdf},
  journal = {Quaternary International},
  language = {en}
}

@article{yorkEvolutionBowerBuilding2015,
  title = {Evolution of Bower Building in {{Lake Malawi}} Cichlid Fish: Phylogeny, Morphology, and Behavior},
  shorttitle = {Evolution of Bower Building in {{Lake Malawi}} Cichlid Fish},
  author = {York, Ryan A. and Patil, Chinar and Hulsey, C. Darrin and Streelman, J. Todd and Fernald, Russell D.},
  year = {2015},
  month = feb,
  volume = {3},
  issn = {2296-701X},
  doi = {10.3389/fevo.2015.00018},
  abstract = {Despite considerable research, we still know little about the proximate and ultimate causes behind behavioral evolution. This is partly because understanding the forces acting on behavioral phenotypes requires the study of species-rich clades with extensive variation in behavioral traits, of which we have few current examples. In this paper, we introduce the bower-building cichlids of the Lake Malawi adaptive radiation, a lineage with over 100 species, each possessing a distinct male extended phenotype used to signal reproductive fitness. Extended phenotypes are useful units of analysis for the study of behavior since they are static structures that can be precisely measured within populations. To this end we recognize two core types of bowers - mounds (``castles'') and depressions (``pits''). We employ an established framework for the study of adaptive radiations to ask how traits related to other stages of radiations, macrohabitat and feeding morphology, are associated with the evolution of pit and castle phenotypes. We demonstrate that pits and castles are evolutionarily labile traits and have been derived numerous times in multiple Malawi genera. Using public ecological and phenotypic data sets we find significant and correlated differences in macrohabitat (depth), sensory ability (opsin expression), and feeding style (jaw morphology and biomechanics) between pit-digging and castle-building species. Phylogeny-corrected comparisons also show significant differences in several measures of jaw morphology while indicating non-significant differences in depth. Finally, using laboratory observations we assay courtship behaviors in a pit-digging (Copadichromis virginalis) and a castle-building species (Mchenga conophoros). Together, these results show that traits at multiple biological levels act to regulate the evolution of a courtship behavior within natural populations.},
  file = {C\:\\Users\\Jon Paige\\Desktop\\Zotero data\\storage\\I77UVHHH\\York et al. - 2015 - Evolution of bower building in Lake Malawi cichlid.pdf},
  journal = {Frontiers in Ecology and Evolution},
  language = {en}
}

