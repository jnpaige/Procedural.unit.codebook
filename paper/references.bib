@article{aubrySiteSolutreenPleinair1998,
  title = {Le Site Solutr\'een de Plein-Air Des {{Maitreaux}} ({{Bossay}}-Sur-{{Claise}}, {{Indre}}-et- {{Loire}}) : Un Faci\`es Original de Production Lithique/{{The}} Open-Air {{Solutrean}} Site of {{Les Maitreaux}} ({{Bossay}}-Sur-{{Claise}}, {{Indre}}-et-{{Loire}}) : An Original Lithic Production Type},
